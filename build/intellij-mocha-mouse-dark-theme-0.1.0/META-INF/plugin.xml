<idea-plugin url="https://github.com/openmindculture/intellij-cute-pink-dark-theme">
    <name>Mocha Mouse Dark Theme</name>
    <id>com.github.openmindculture.intellijmochamousedarktheme</id>
    <description>Mocha Mouse Dark Theme for IntelliJ Platform is inspired by Mocha Mousse, Pantone's Color of the Year 2025, based on openmindculture's Cute Pink Dark Theme.</description>
    <change-notes>Initial theme release. Basic theme functionality.</change-notes>
    <version>0.1.0</version>
    <vendor url="https://github.com/openmindculture/">Ingo Steinke (openmindculture)</vendor>

    <!-- Compatible with the following versions of IntelliJ Platform:
      version 2020.2 and newer. -->
    <idea-version since-build="202" />

    <!-- Product and plugin compatibility requirements -->
    <!-- https://plugins.jetbrains.com/docs/intellij/plugin-compatibility.html -->
    <!-- Indicate this plugin can be loaded in all IntelliJ Platform-based products. -->
    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <themeProvider id="org.jetbrains.plugins.mocha_mouse_darktheme" path="/mocha_mouse_dark.theme.json"/>
    </extensions>
</idea-plugin>
